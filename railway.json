{
  "builds": [
    {
      "src": "internet_server.py",
      "use": "@railway/python"
    }
  ],
  "deploy": {
    "startCommand": "python internet_server.py",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
